{
  "version": 1,
  "rules": [
    {
      "regexp": "Review transaction|From|Amount|To|Fees",
      "actions": [
        ["button", 2, true],
        ["button", 2, false]
      ]
    },
    {
      "regexp": "Continue|Sign transaction",
      "actions": [
        [ "button", 1, true ],
        [ "button", 2, true ],
        [ "button", 2, false ],
        [ "button", 1, false ]
      ]
    }
  ]
}
