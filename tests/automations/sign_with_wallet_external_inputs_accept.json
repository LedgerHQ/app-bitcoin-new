{
  "version": 1,
  "rules": [
    {
      "regexp": "Hold to sign",
      "actions": [
        ["finger", 55, 550, true]
      ]
    },
    {
      "regexp": "Spend from|Wallet name|There are|Reject if you['-]re|Review|Amount|Address|Confirm|Fees",
      "actions": [
        ["button", 2, true],
        ["button", 2, false]
      ]
    },
    {
      "regexp": "Tap to continue|Warning",
      "actions": [
        ["finger", 55, 550, true],
        ["finger", 55, 550, false]
      ]
    },
    {
      "regexp": "Continue|Approve|Accept",
      "actions": [
        [ "button", 1, true ],
        [ "button", 2, true ],
        [ "button", 2, false ],
        [ "button", 1, false ],
        [ "finger", 55, 550, true]
      ]
    },
    {
      "regexp": "SIGNED",
      "actions": [
        ["finger", 55, 550, false]
      ]
    }
  ]
}
