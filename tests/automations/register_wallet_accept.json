{
  "version": 1,
  "rules": [
    {
      "regexp": "Register wallet|Wallet name|Wallet policy|Key",
      "actions": [
        ["button", 2, true],
        ["button", 2, false],
        [ "finger", 55, 550, true],
        [ "finger", 55, 550, false]
      ]
    },
    {
      "regexp": "Tap to continue",
      "actions": [
        ["finger", 55, 550, true],
        ["finger", 55, 550, false]
      ]
    },
    {
      "text": "Approve",
      "actions": [
        [ "button", 1, true ],
        [ "button", 2, true ],
        [ "button", 2, false ],
        [ "button", 1, false ],
        [ "finger", 55, 550, true]
      ]
    },
    {
      "regexp": "Processing|REGISTERED",
      "actions": [
        ["finger", 55, 550, false]
      ]
    }
  ]
}
