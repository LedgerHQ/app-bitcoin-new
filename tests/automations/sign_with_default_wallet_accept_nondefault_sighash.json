{
  "version": 1,
  "rules": [
    {
      "regexp": "Review|Amount|Address|Confirm|Fees|Non-default sighash|Reject",
      "actions": [
        ["button", 2, true],
        ["button", 2, false]
      ]
    },
    {
      "regexp": "Tap to continue|Warning",
      "actions": [
        ["finger", 55, 550, true],
        ["finger", 55, 550, false]
      ]
    },
    {
      "regexp": "Hold|Accept|Continue|Approve",
      "actions": [
        [ "button", 1, true ],
        [ "button", 2, true ],
        [ "button", 2, false ],
        [ "button", 1, false ],
        [ "finger", 55, 550, true]
      ]
    },
    {
      "regexp": "SIGNED",
      "actions": [
        ["finger", 55, 550, false]
      ]
    }
  ]
}
