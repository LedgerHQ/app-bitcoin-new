{
  "version": 1,
  "rules": [
    {
      "regexp": "Tap to continue",
      "actions": [
        ["finger", 55, 550, true],
        ["finger", 55, 550, false]
      ]
    },
    {
      "regexp": "[S]?ign|Path|Message hash|Approve",
      "actions": [
        ["button", 2, true],
        ["button", 2, false],
        ["finger", 55, 650, true],
        ["finger", 55, 650, false]
      ]
    },
    {
      "regexp": "Reject",
      "actions": [
        [ "button", 1, true ],
        [ "button", 2, true ],
        [ "button", 2, false ],
        [ "button", 1, false ]
      ]
    }
  ]
}
